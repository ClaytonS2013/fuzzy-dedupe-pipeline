{
  "ai_features": {
    "semantic_matching": true,
    "claude_validation": true,
    "embedding_model": "all-MiniLM-L6-v2",
    "similarity_threshold": 0.80,
    "batch_size": 100,
    "max_candidates": 10
  },
  "models": {
    "sentence_transformer": {
      "name": "all-MiniLM-L6-v2",
      "dimension": 384,
      "cache_dir": "/app/models"
    },
    "claude": {
      "model": "claude-3-sonnet-20240229",
      "max_tokens": 500,
      "temperature": 0.1
    }
  },
  "processing": {
    "use_gpu": false,
    "num_workers": 4,
    "chunk_size": 1000
  }
}
